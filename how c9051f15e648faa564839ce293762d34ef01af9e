[36m@@ -0,0 +1,23 @@[m
[32m+[m[32m'use server';[m
[32m+[m
[32m+[m[32mimport bcrypt from 'bcrypt';[m
[32m+[m[32mimport { pool } from '@/app/lib/db';[m
[32m+[m
[32m+[m[32mexport async function signupAction({ name, email, password }: { name: string; email: string; password: string }) {[m
[32m+[m[32m  if (!name || !email || !password) {[m
[32m+[m[32m    throw new Error('All fields are required.');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  const { rows } = await pool.query('SELECT * FROM users WHERE email = $1', [email]);[m
[32m+[m[32m  if (rows.length > 0) {[m
[32m+[m[32m    throw new Error('User already exists.');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  const hashedPassword = await bcrypt.hash(password, 10);[m
[32m+[m[32m  const result = await pool.query([m
[32m+[m[32m    'INSERT INTO users (name, email, password) VALUES ($1, $2, $3) RETURNING id, name, email',[m
[32m+[m[32m    [name, email, hashedPassword][m
[32m+[m[32m  );[m
[32m+[m
[32m+[m[32m  return result.rows[0];[m
[32m+[m[32m}[m
