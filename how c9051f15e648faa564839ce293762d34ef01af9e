[1mdiff --git a/app/pages/api/signup.tsx b/app/pages/api/signup.tsx[m
[1mnew file mode 100644[m
[1mindex 0000000..d3df204[m
[1m--- /dev/null[m
[1m+++ b/app/pages/api/signup.tsx[m
[36m@@ -0,0 +1,48 @@[m
[32m+[m[32mimport { NextApiRequest, NextApiResponse } from 'next';[m
[32m+[m[32mimport bcrypt from 'bcrypt';[m
[32m+[m[32mimport { pool } from '../../lib/db'; // Database connection pool[m
[32m+[m
[32m+[m[32m// Type for incoming data[m
[32m+[m[32minterface SignupData {[m
[32m+[m[32m  name: string;[m
[32m+[m[32m  email: string;[m
[32m+[m[32m  password: string;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mexport default async function handler(req: NextApiRequest, res: NextApiResponse) {[m
[32m+[m[32m  if (req.method === 'POST') {[m
[32m+[m[32m    const { name, email, password }: SignupData = req.body;[m
[32m+[m
[32m+[m[32m    // Basic validation[m
[32m+[m[32m    if (!name || !email || !password) {[m
[32m+[m[32m      return res.status(400).json({ error: 'All fields are required.' });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    try {[m
[32m+[m[32m      // Check if user already exists[m
[32m+[m[32m      const { rows } = await pool.query('SELECT * FROM users WHERE email = $1', [email]);[m
[32m+[m[32m      if (rows.length > 0) {[m
[32m+[m[32m        return res.status(400).json({ error: 'User already exists.' });[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      // Hash the password[m
[32m+[m[32m      const hashedPassword = await bcrypt.hash(password, 10);[m
[32m+[m
[32m+[m[32m      // Insert user into database[m
[32m+[m[32m      const result = await pool.query([m
[32m+[m[32m        'INSERT INTO users (name, email, password) VALUES ($1, $2, $3) RETURNING id, name, email',[m
[32m+[m[32m        [name, email, hashedPassword][m
[32m+[m[32m      );[m
[32m+[m
[32m+[m[32m      const newUser = result.rows[0];[m
[32m+[m
[32m+[m[32m      return res.status(201).json({ user: newUser });[m
[32m+[m[32m    } catch (error) {[m
[32m+[m[32m      console.error('Error during signup:', error);[m
[32m+[m[32m      return res.status(500).json({ error: 'An error occurred during signup.' });[m
[32m+[m[32m    }[m
[32m+[m[32m  } else {[m
[32m+[m[32m    // Method not allowed[m
[32m+[m[32m    return res.status(405).json({ error: 'Method Not Allowed' });[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
