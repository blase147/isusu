[32m+[m[32mimport { NextAuthOptions } from "next-auth";[m
[32m+[m[41m [m
[32m+[m[32mimport CredentialsProvider from "next-auth/providers/credentials";[m
[32m+[m
[32m+[m[32mexport const authConfig: NextAuthOptions = {[m
[32m+[m[32m  pages: {[m
[32m+[m[32m    signIn: '/login',[m
[32m+[m[32m  },[m
[32m+[m[32m  callbacks: {[m
[32m+[m[32m    async authorized({ auth, request: { nextUrl } }: { auth: any, request: { nextUrl: URL } }) {[m
[32m+[m[32m      const isLoggedIn = !!auth?.user;[m
[32m+[m[32m      const isOnDashboard = nextUrl.pathname.startsWith('/dashboard');[m
[32m+[m
[32m+[m[32m      if (isOnDashboard) {[m
[32m+[m[32m        return isLoggedIn;[m
[32m+[m[32m      } else if (isLoggedIn) {[m
[32m+[m[32m        return Response.redirect(new URL('/dashboard', nextUrl));[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      return true;[m
[32m+[m[32m    },[m
[32m+[m[32m  },[m
[32m+[m[32m  providers: [[m
[32m+[m[32m    CredentialsProvider({[m
[32m+[m[32m      name: "Credentials",[m
[32m+[m[32m      credentials: {[m
[32m+[m[32m        username: { label: "Username", type: "text" },[m
[32m+[m[32m        password: { label: "Password", type: "password" },[m
[32m+[m[32m      },[m
[32m+[m[32m      async authorize(credentials) {[m
[32m+[m[32m        // Implement your authentication logic here[m
[32m+[m[32m        const user = { id: 1, name: "Test User", email: "test@example.com" }; // Dummy data[m
[32m+[m[32m        if (user) {[m
[32m+[m[32m          return user;[m
[32m+[m[32m        }[m
[32m+[m[32m        return null;[m
[32m+[m[32m      },[m
[32m+[m[32m    }),[m
[32m+[m[32m  ],[m
[32m+[m[32m};[m
