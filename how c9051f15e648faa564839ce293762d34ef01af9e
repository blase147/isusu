[1mdiff --git a/app/dashboard/query/route.ts b/app/dashboard/query/route.ts[m
[1mnew file mode 100644[m
[1mindex 0000000..2646234[m
[1m--- /dev/null[m
[1m+++ b/app/dashboard/query/route.ts[m
[36m@@ -0,0 +1,31 @@[m
[32m+[m[32mimport { db } from '@vercel/postgres';[m
[32m+[m[32mimport { NextResponse } from 'next/server'; // Correct import for Next.js responses[m
[32m+[m
[32m+[m[32masync function getClient() {[m
[32m+[m[32m  const client = await db.connect();[m
[32m+[m[32m  return client;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32masync function listInvoices(client) {[m
[32m+[m[32m  const data = await client.sql`[m
[32m+[m[32m    SELECT invoices.amount, customers.name[m
[32m+[m[32m    FROM invoices[m
[32m+[m[32m    JOIN customers ON invoices.customer_id = customers.id[m
[32m+[m[32m    WHERE invoices.amount = 666;[m
[32m+[m[32m  `;[m
[32m+[m[32m  return data.rows;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mexport async function GET() {[m
[32m+[m[32m  const client = await getClient(); // Initialize the database connection[m
[32m+[m[32m  try {[m
[32m+[m[32m    const invoices = await listInvoices(client); // Fetch data[m
[32m+[m[32m    return NextResponse.json(invoices); // Respond with the data[m
[32m+[m[32m  } catch (error) {[m
[32m+[m[32m    console.error('Error fetching invoices:', error); // Log the error[m
[32m+[m[32m    const errorMessage = error instanceof Error ? error.message : 'Unknown error';[m
[32m+[m[32m    return NextResponse.json({ error: errorMessage }, { status: 500 }); // Return error response[m
[32m+[m[32m  } finally {[m
[32m+[m[32m    client.release(); // Ensure the database connection is released[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
