[1mdiff --git a/app/ui/signup-form.tsx b/app/ui/signup-form.tsx[m
[1mnew file mode 100644[m
[1mindex 0000000..0493177[m
[1m--- /dev/null[m
[1m+++ b/app/ui/signup-form.tsx[m
[36m@@ -0,0 +1,81 @@[m
[32m+[m[32m'use client';[m
[32m+[m
[32m+[m[32mimport { useState } from 'react';[m
[32m+[m[32mimport { signupAction } from '@/app/signup';[m
[32m+[m
[32m+[m[32mexport default function SignupForm() {[m
[32m+[m[32m  const [signup, setSignup] = useState({ name: '', email: '', password: '' });[m
[32m+[m
[32m+[m[32m  const handleChange = (e: React.ChangeEvent<HTMLInputElement>) => {[m
[32m+[m[32m    const { name, value } = e.target;[m
[32m+[m[32m    setSignup((prev) => ({ ...prev, [name]: value }));[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  const handleSubmit = async (e: React.FormEvent) => {[m
[32m+[m[32m    e.preventDefault();[m
[32m+[m[32m    try {[m
[32m+[m[32m      await signupAction(signup);[m
[32m+[m[32m      alert('Signup successful!');[m
[32m+[m[32m    } catch (error) {[m
[32m+[m[32m      alert((error as Error).message || 'An error occurred');[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return ([m
[32m+[m[32m    <div className="flex items-center justify-center min-h-screen bg-gray-50">[m
[32m+[m[32m      <div className="w-full max-w-md p-8 bg-white rounded-lg shadow-md">[m
[32m+[m[32m        <h1 className="mb-6 text-2xl font-bold text-center text-gray-800">Signup</h1>[m
[32m+[m[32m        <form onSubmit={handleSubmit} className="space-y-4">[m
[32m+[m[32m          <div>[m
[32m+[m[32m            <label htmlFor="name" className="block text-sm font-medium text-gray-700">[m
[32m+[m[32m              Name[m
[32m+[m[32m            </label>[m
[32m+[m[32m            <input[m
[32m+[m[32m              type="text"[m
[32m+[m[32m              id="name"[m
[32m+[m[32m              name="name"[m
[32m+[m[32m              value={signup.name}[m
[32m+[m[32m              onChange={handleChange}[m
[32m+[m[32m              required[m
[32m+[m[32m              className="w-full px-3 py-2 mt-1 border border-gray-300 rounded-md shadow-sm focus:outline-none focus:ring-blue-500 focus:border-blue-500"[m
[32m+[m[32m            />[m
[32m+[m[32m          </div>[m
[32m+[m[32m          <div>[m
[32m+[m[32m            <label htmlFor="email" className="block text-sm font-medium text-gray-700">[m
[32m+[m[32m              Email[m
[32m+[m[32m            </label>[m
[32m+[m[32m            <input[m
[32m+[m[32m              type="email"[m
[32m+[m[32m              id="email"[m
[32m+[m[32m              name="email"[m
[32m+[m[32m              value={signup.email}[m
[32m+[m[32m              onChange={handleChange}[m
[32m+[m[32m              required[m
[32m+[m[32m              className="w-full px-3 py-2 mt-1 border border-gray-300 rounded-md shadow-sm focus:outline-none focus:ring-blue-500 focus:border-blue-500"[m
[32m+[m[32m            />[m
[32m+[m[32m          </div>[m
[32m+[m[32m          <div>[m
[32m+[m[32m            <label htmlFor="password" className="block text-sm font-medium text-gray-700">[m
[32m+[m[32m              Password[m
[32m+[m[32m            </label>[m
[32m+[m[32m            <input[m
[32m+[m[32m              type="password"[m
[32m+[m[32m              id="password"[m
[32m+[m[32m              name="password"[m
[32m+[m[32m              value={signup.password}[m
[32m+[m[32m              onChange={handleChange}[m
[32m+[m[32m              required[m
[32m+[m[32m              className="w-full px-3 py-2 mt-1 border border-gray-300 rounded-md shadow-sm focus:outline-none focus:ring-blue-500 focus:border-blue-500"[m
[32m+[m[32m            />[m
[32m+[m[32m          </div>[m
[32m+[m[32m          <button[m
[32m+[m[32m            type="submit"[m
[32m+[m[32m            className="w-full px-4 py-2 text-white bg-blue-600 rounded-md shadow hover:bg-blue-700 focus:outline-none focus:ring-2 focus:ring-blue-500 focus:ring-offset-2"[m
[32m+[m[32m          >[m
[32m+[m[32m            Signup[m
[32m+[m[32m          </button>[m
[32m+[m[32m        </form>[m
[32m+[m[32m      </div>[m
[32m+[m[32m    </div>[m
[32m+[m[32m  );[m
[32m+[m[32m}[m
