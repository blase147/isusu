[32m+[m[32mimport Link from 'next/link';[m
[32m+[m[32mimport NavLinks from '@/app/ui/dashboard/nav-links';[m
[32m+[m[32mimport AcmeLogo from '@/app/ui/acme-logo';[m
[32m+[m[32mimport { PowerIcon } from '@heroicons/react/24/outline';[m
[32m+[m[32mimport { signOut } from '@/auth';[m
