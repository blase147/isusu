[1mdiff --git a/auth.ts b/auth.ts[m
[1mnew file mode 100644[m
[1mindex 0000000..6b03d7e[m
[1m--- /dev/null[m
[1m+++ b/auth.ts[m
[36m@@ -0,0 +1,54 @@[m
[32m+[m[32mimport NextAuth from 'next-auth';[m
[32m+[m[32mimport Credentials from 'next-auth/providers/credentials';[m
[32m+[m[32mimport { authConfig } from './auth.config';[m
[32m+[m[32mimport { z } from 'zod';[m
[32m+[m[32mimport { sql } from '@vercel/postgres';[m
[32m+[m[32mimport type { User } from '@/app/lib/definitions';[m
[32m+[m[32mimport bcrypt from 'bcrypt';[m
[32m+[m[41m [m
[32m+[m[32masync function getUser(email: string): Promise<User | undefined> {[m
[32m+[m[32m  try {[m
[32m+[m[32m    const user = await sql<User>`SELECT * FROM users WHERE email=${email}`;[m
[32m+[m[32m    return user.rows[0];[m
[32m+[m[32m  } catch (error) {[m
[32m+[m[32m    console.error('Failed to fetch user:', error);[m
[32m+[m[32m    throw new Error('Failed to fetch user.');[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[41m [m
[32m+[m[32mexport const { auth, signIn, signOut } = NextAuth({[m
[32m+[m[32m  ...authConfig,[m
[32m+[m[32m  providers: [[m
[32m+[m[32m    Credentials({[m
[32m+[m[32masync authorize(credentials) {[m
[32m+[m[32m  const parsedCredentials = z[m
[32m+[m[32m    .object({ email: z.string().email(), password: z.string().min(6) })[m
[32m+[m[32m    .safeParse(credentials);[m
[32m+[m
[32m+[m[32m  if (!parsedCredentials.success) {[m
[32m+[m[32m    console.log('Invalid input format:', parsedCredentials.error);[m
[32m+[m[32m    return null;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  const { email, password } = parsedCredentials.data;[m
[32m+[m[32m  console.log('Parsed credentials:', { email, password });[m
[32m+[m
[32m+[m[32m  const user = await getUser(email);[m
[32m+[m[32m  if (!user) {[m
[32m+[m[32m    console.log('No user found for email:', email);[m
[32m+[m[32m    return null;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  console.log('User found:', user);[m
[32m+[m
[32m+[m[32m  const passwordsMatch = await bcrypt.compare(password, user.password);[m
[32m+[m[32m  console.log('Passwords match:', passwordsMatch);[m
[32m+[m
[32m+[m[32m  if (passwordsMatch) return user;[m
[32m+[m
[32m+[m[32m  console.log('Password did not match for user:', email);[m
[32m+[m[32m  return null;[m
[32m+[m[32m}[m
[32m+[m[32m    }),[m
[32m+[m[32m  ],[m
[32m+[m[32m});[m
\ No newline at end of file[m
[1mdiff --git a/eslint.config.mjs b/eslint.config.mjs[m
[1mnew file mode 100644[m
[1mindex 0000000..c85fb67[m
[1m--- /dev/null[m
[1m+++ b/eslint.config.mjs[m
[36m@@ -0,0 +1,16 @@[m
[32m+[m[32mimport { dirname } from "path";[m
[32m+[m[32mimport { fileURLToPath } from "url";[m
[32m+[m[32mimport { FlatCompat } from "@eslint/eslintrc";[m
[32m+[m
[32m+[m[32mconst __filename = fileURLToPath(import.meta.url);[m
[32m+[m[32mconst __dirname = dirname(__filename);[m
[32m+[m
[32m+[m[32mconst compat = new FlatCompat({[m
[32m+[m[32m  baseDirectory: __dirname,[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mconst eslintConfig = [[m
[32m+[m[32m  ...compat.extends("next/core-web-vitals", "next/typescript"),[m
[32m+[m[32m];[m
[32m+[m
[32m+[m[32mexport default eslintConfig;[m
