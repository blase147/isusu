[1mnew file mode 100644[m
[1mindex 0000000..1fdda5b[m
[1m--- /dev/null[m
[1m+++ b/middleware.ts[m
[36m@@ -0,0 +1,9 @@[m
[32m+[m[32mimport NextAuth from 'next-auth';[m
[32m+[m[32mimport { authConfig } from './auth.config';[m
[32m+[m[41m [m
[32m+[m[32mexport default NextAuth(authConfig).auth;[m
[32m+[m[41m [m
[32m+[m[32mexport const config = {[m
[32m+[m[32m  // https://nextjs.org/docs/app/building-your-application/routing/middleware#matcher[m
[32m+[m[32m  matcher: ['/((?!api|_next/static|_next/image|.*\\.png$).*)'],[m
[32m+[m[32m};[m
